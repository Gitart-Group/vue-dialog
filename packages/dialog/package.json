{
  "name": "gitart-vue-dialog",
  "version": "1.2.1",
  "scripts": {
    "dev": "vite --config dev/vite.config.ts",
    "build": "node build/build.js",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --p ./build/tsconfig.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "gitart",
    "gitart-vue-dialog",
    "vue",
    "dialog",
    "modal",
    "dialog component",
    "modal component",
    "vue plugin",
    "vue modal",
    "vue dialog"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitart-vue-dialog.gitart.org",
    "directory": "packages/dialog"
  },
  "author": {
    "name": "Michael Gitart",
    "email": "michaelgitart@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.6.0",
    "sass": "^1.39.0",
    "shelljs": "^0.8.4",
    "unplugin-auto-import": "^0.5.1",
    "unplugin-vue-components": "^0.17.5",
    "vite": "^2.5.3",
    "vite-plugin-windicss": "^1.5.4",
    "vue": "3.2.6",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.2.1"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  }
}
